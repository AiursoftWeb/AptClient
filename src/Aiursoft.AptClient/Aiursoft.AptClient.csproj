<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>10.0.1</Version>
    <TargetFramework>net10.0</TargetFramework>
    <AssemblyName>Aiursoft.AptClient</AssemblyName>
    <RootNamespace>Aiursoft.AptClient</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>AptClient</Product>
    <Authors>anduin</Authors>
    <Description>Automatic dependencies management system which powers Aiursoft</Description>
    <PackageId>Aiursoft.AptClient</PackageId>
    <PackageTags>dependencies injection aptClient</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://gitlab.aiursoft.com/aiursoft/aptClient</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://gitlab.aiursoft.com/aiursoft/aptClient</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.1" />
    <None Include="../../README.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Aiursoft.AptClient.Abstractions\Aiursoft.AptClient.Abstractions.csproj" />
  </ItemGroup>
</Project>
